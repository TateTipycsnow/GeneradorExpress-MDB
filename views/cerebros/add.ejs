<!--VISTA FORMULARIO CEREBROS-->

<% include ../_header %>
 
<div class="row container">
    <div class="col-md-12">
        <form action="new" method="POST">
            <div class="form-group">
                <label for="">Descripcion</label>
                <input type="text" class="form-control" name="description">
            </div>
            <div class="form-group">
                <label for="">Sabor</label>
                <input type="text" class="form-control" name="flavor">
            </div>
            <div class="form-group">
                <label for="">Precio</label>
                <input type="text" class="form-control" name="price">
            </div>
            <div class="form-group">
                <label for="">Imagen</label>
                <input type="text" class="form-control" name="picture">
            </div>
            <button class="btn btn-dark">
                <i class="fas fa-brain"></i>
                Nuevo cerebro
            </button>
        </form>
        
        <div class= " <%= Alerta %> " role="alert" <%= Hide %>>
            <h6 class="alert-heading"> <%= Mensaje %>  </h6>
        </div>

    </div>
</div>

<% include ../_footer %>